{
    "name": "labRats",
    "description": "Anti Phishing",
    "version": "1.0",
    "manifest_version": 3,
    "host_permissions" : [
    "http://*/*",
    "https://*/*"
    ],
    "permissions" : [
        "webRequest"
    ],
    "content_scripts": [
        {
            "js": [
                "scripts/content.js"
            ],
            "matches": [
                "*://*/*"
            ]
        }
    ]
}